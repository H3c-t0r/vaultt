[ req ]
default_bits = 2048
encrypt_key = no
prompt = no
default_md = sha256
req_extensions = req_v3
distinguished_name = dn

[ dn ]
UID = TheUID
CN = example.com
C = US
ST = CA
L = Sunnyvale
O = ExampleOrg
0.OU = ExampleDivision1
1.OU = ExampleDivision2
2.OU = ExampleDivision3
DC = ExampleDC
SN = ExampleSN

[ req_v3 ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = 127.0.0.1
DNS.1 = example.com
