<div class="console-ui-output">
  <pre>{{columns}}</pre>

  <div class="console-ui-output-actions">
    {{#tool-tip renderInPlace=true as |d|}}
      {{#d.trigger tagName="button" type="button" class=(concat "button is-compact") data-test-tool-tip-trigger=true}}
        {{i-con glyph="copy" aria-hidden="true" size=16}}
      {{/d.trigger}}
      {{#d.content class="tool-tip"}}
        <div class="box">
          Copy
        </div>
      {{/d.content}}
    {{/tool-tip}}
  </div>
</div>
