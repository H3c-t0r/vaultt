{{#unless this.dontShowTab}}
  {{#if @isEngine}}
    <LinkTo @route={{@link}} data-test-secret-list-tab={{@label}}>
      {{@label}}
    </LinkTo>
  {{else}}
    <LinkTo @route="vault.cluster.secrets.backend.list-root" @query={{hash tab=@tab}} data-test-secret-list-tab={{@label}}>
      {{@label}}
    </LinkTo>
  {{/if}}
{{/unless}}