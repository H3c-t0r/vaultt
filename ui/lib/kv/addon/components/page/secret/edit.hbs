<PageHeader as |p|>
  <p.top>
    <Page::Breadcrumbs @breadcrumbs={{@breadcrumbs}} />
  </p.top>
  <p.levelLeft>
    <h1 class="title is-3">
      Create New Version
    </h1>
  </p.levelLeft>
</PageHeader>

<KvSecretForm
  @previousVersion={{@previousVersion}}
  @metadata={{@metadata}}
  @secret={{@newVersion}}
  {{! onSave transition to parent secret route to re-query latest version }}
  @onSave={{transition-to "vault.cluster.secrets.backend.kv.secret"}}
  @onCancel={{transition-to "vault.cluster.secrets.backend.kv.secret.details"}}
/>